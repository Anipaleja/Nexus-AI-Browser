{
  "name": "nexus-ai-browser",
  "version": "1.0.0",
  "description": "An AI-powered browser that provides hyper-personalized experiences through real-time data analysis and machine learning",
  "main": "build/main.js",
  "homepage": "https://nexus-browser.ai",
  "author": {
    "name": "Anish Paleja",
    "email": "anish@nexus-browser.ai"
  },
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" --kill-others",
    "dev:main": "webpack --config webpack.main.js --mode development --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.js --mode development",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.js --mode production",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "browser",
    "personalization",
    "machine-learning",
    "electron",
    "real-time",
    "intelligent"
  ],
  "author": "Anish Paleja",
  "license": "MIT",
  "build": {
    "appId": "com.anishpaleja.nexus-browser",
    "productName": "Nexus AI Browser",
    "copyright": "Copyright Â© 2024 Anish Paleja",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "!node_modules/.cache",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets/",
        "filter": ["**/*"]
      }
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "dmg": {
      "title": "Nexus AI Browser",
      "icon": "assets/icon.icns",
      "background": "assets/dmg-background.png",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "portable",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Nexus AI Browser"
    },
    "linux": {
      "icon": "assets/icon.png",
      "category": "Network",
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64"]
        },
        {
          "target": "deb",
          "arch": ["x64"]
        },
        {
          "target": "rpm",
          "arch": ["x64"]
        }
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "anishpaleja",
      "repo": "nexus-ai-browser"
    }
  },
  "dependencies": {
    "electron": "^28.0.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@tensorflow/tfjs": "^4.15.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "chrono-node": "^2.7.3",
    "compromise": "^14.12.0",
    "crypto-js": "^4.2.0",
    "dexie": "^3.2.4",
    "express": "^4.18.2",
    "fuse.js": "^7.0.0",
    "languagedetect": "^2.0.0",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.6",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@types/node": "^20.10.4",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}

