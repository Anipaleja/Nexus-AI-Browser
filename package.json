{
  "name": "nexus-ai-browser",
  "version": "1.0.0",
  "description": "An AI-powered browser that provides hyper-personalized experiences through real-time data analysis and machine learning",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "nodemon --exec electron . --ignore src/renderer/",
    "dev:renderer": "webpack serve --config webpack.renderer.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.js",
    "build:renderer": "webpack --config webpack.renderer.js",
    "pack": "electron-builder",
    "dist": "npm run build && electron-builder",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "ai",
    "browser",
    "personalization",
    "machine-learning",
    "electron",
    "real-time",
    "intelligent"
  ],
  "author": "Anish Paleja",
  "license": "MIT",
  "build": {
    "appId": "com.anishpaleja.nexus-browser",
    "productName": "Nexus AI Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "category": "public.app-category.productivity"
    },
    "win": {
      "icon": "assets/icon.ico"
    },
    "linux": {
      "icon": "assets/icon.png"
    }
  },
  "dependencies": {
    "electron": "^28.0.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@tensorflow/tfjs": "^4.15.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "chrono-node": "^2.7.3",
    "compromise": "^14.12.0",
    "crypto-js": "^4.2.0",
    "dexie": "^3.2.4",
    "express": "^4.18.2",
    "fuse.js": "^7.0.0",
    "languagedetect": "^2.0.0",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.6",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@types/node": "^20.10.4",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}

